    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "3.8.3",
    "url-loader": "1.1.1",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "xcode": "3.0.1",
    "yargs": "16.0.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run tsc",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "stylelint",
      "prettier --write",
      "eslint --fix"
