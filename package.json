    "eslint": "6.7.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "4.0.5",
    "husky": "1.3.1",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.3",
    "jetifier": "1.6.4",
    "lint-staged": "10.0.8",
    "metro": "0.57.0",
    "metro-react-native-babel-preset": "0.66.2",
    "patch-package": "6.4.7",
