{
  "name": "ReactNativeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pod:clean": "rm -rf ios/Pods && cd ios && pod install && cd ../",
    "android": "react-native run-android",
    "ios": "rm -rf ios/build && npm run pod:clean && npm run copyReactNativeFiles && react-native run-ios --simulator='iPhone 13 Pro Max'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint './src/**/*.{ts,tsx,json}'",
    "copyReactNativeFiles": "chmod +x ./copyReactNativeFiles.sh && bash ./copyReactNativeFiles.sh",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,json}'"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.0",
    "@types/history": "5.0.0",
    "axios": "0.21.0",
    "color": "3.2.1",
    "flux-standard-action": "^2.0.3",
    "history": "4.10.1",
    "native-base": "^3.4.22",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-device-info": "8.4.8",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "1.10.3",
    "react-native-reanimated": "~1.9.0",
    "react-native-router-flux": "4.0.3",
